{
  "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",
  "rushVersion": "5.113.4",
  "pnpmVersion": "7.33.5",
  "nodeSupportedVersionRange": ">=20.0.0",
  "suppressNodeLtsWarning": true,
  "repository": {
    "url": "https://github.com/SonyaHon/joy.git",
    "defaultBranch": "main",
    "defaultRemote": "origin"
  },
  "projects": [
    {
      "packageName": "@joy/prettier-config",
      "projectFolder": "config/prettier",
      "tags": ["config"]
    },
    {
      "packageName": "@joy/eslint-config",
      "projectFolder": "config/eslint",
      "tags": ["config"]
    },
    {
      "packageName": "@joy/engine",
      "projectFolder": "engine",
      "tags": ["core"]
    },
    {
      "packageName": "@joy/natives",
      "projectFolder": "natives",
      "tags": ["core"]
    },
    {
      "packageName": "@joy/sandbox",
      "projectFolder": "sandbox",
      "tags": ["sandbox"]
    }
  ]
}
